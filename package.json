{
  "name": "empmanagementui",
  "description": "empManagementUI",
  "author": "Gaurav Sikka",
  "private": true,
  "version": "0.4.6",
  "license": "MIT",
  "scripts": {
    "build:client": "cross-env NODE_ENV=production webpack --mode=development --config webpack.client.js --watch --progress",
    "build:server": "cross-env NODE_ENV=production webpack --mode=development --config webpack.server.js --watch --progress",
    "start": "nodemon -r dotenv/config --icu-data-dir=node_modules/full-icu dist/server.bundle.js",
    "build": "concurrently \"npm:build:client\" \"npm:build:server\"",
    "buildprod:client": "cross-env NODE_ENV=production webpack --mode=production --config webpack.client.js",
    "buildprod:server": "cross-env NODE_ENV=production webpack --mode=production --config webpack.server.js",
    "buildprod": "npm run standard && npm run flow && npm test && npm run buildprod:client && npm run buildprod:server",
    "test": "jest --no-cache --coverage",
    "standard": "standard --verbose | snazzy",
    "standard:fix": "standard --fix",
    "flow": "flow",
    "format": "prettier-standard 'src/**/*.{js,jsx}'"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier-standard",
      "standard --fix",
      "git add"
    ]
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "globals": {
      "__APPVERSION__": "9.9.9",
      "__CLIENT__": true
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/__tests__/**"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "cobertura"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 40,
        "functions": 40,
        "lines": 40,
        "statements": 40
      }
    },
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "fbjs",
      "enzyme",
      "lodash"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "react-lottie": "<rootDir>/__mocks__/lottieMock.js"
    },
    "roots": [
      "<rootDir>/__tests__/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "ignore": [
      "__mocks__",
      "__tests__",
      "dist",
      "coverage",
      "migrations",
      "src/client/constants",
      "src/setupTests.js"
    ],
    "globals": [
      "__CLIENT__",
      "Headers",
      "FileReader",
      "__APPVERSION__",
      "alert",
      "localStorage",
      "sessionStorage",
      "$logger",
      "fetch"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-flow": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-stage-0": "^7.8.3",
    "@redux-saga/testing-utils": "^1.1.3",
    "autoprefixer": "^9.8.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^6.0.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "*",
    "eslint": "^7",
    "eslint-plugin-flowtype": "^5.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fetch-mock": "^9.9.0",
    "file-loader": "^6.0.0",
    "flow-bin": "^0.125.1",
    "global-request-logger": "^0.1.1",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-cli": "^26.0.1",
    "less": "^3.11.1",
    "less-loader": "^6.1.0",
    "lint-staged": "^10.2.4",
    "mini-css-extract-plugin": "^0.9.0",
    "node-pg-migrate": "^4.8.0",
    "node-sass": "^7.0.0",
    "nodemon": "^2.0.4",
    "postcss-loader": "^3.0.0",
    "prettier-standard": "^16.3.0",
    "raw-loader": "^4.0.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^8.0.2",
    "snazzy": "^8.0.0",
    "standard": "*",
    "style-loader": "^1.2.1",
    "supertest": "^4.0.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "@hapi/joi": "^17.1.1",
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "beaver-logger": "^4.0.14",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-pg-simple": "^6.1.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "full-icu": "^1.3.1",
    "helmet": "^3.22.0",
    "http-proxy-middleware": "^1.0.4",
    "isomorphic-fetch": "^2.2.1",
    "keymirror": "^0.1.1",
    "lodash": "^4.17.15",
    "moment": "^2.25.3",
    "pg": "^8.2.1",
    "print-js": "^1.0.63",
    "prop-types": "^15.7.2",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-data-grid": "^6.1.0",
    "react-dom": "^16.13.1",
    "react-intl": "^4.5.5",
    "react-loadable": "^5.5.0",
    "react-lottie": "^1.2.3",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "save": "^2.4.0",
    "serve-favicon": "^2.5.0",
    "typeface-lato": "0.0.75",
    "uuid": "^8.0.0",
    "winston": "^3.2.1",
    "react-redux-popup": "git+https://github.com/msikka93/react-redux-popup.git",
    "react-redux-snackbar": "git+https://github.com/msikka93/react-redux-snackbar.git",
    "react-hook-form": "^5.7.2"
  }
}
